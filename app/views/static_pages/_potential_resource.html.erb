<div class="resources col-md-6">
  <div style="color: #736f7e">
    <h2>Questionnaires</h2>
      <h3>
        <% if current_user.interview_questionnaires.any? %>
        <%= link_to "Questionnaire", edit_interview_questionnaire_path(current_user.interview_questionnaires.last.id) %>
            <% else %>
        <p>Please Pick a Time Slot</p>
        <% end %>
      </h3>
  </div>
  <hr>
</div>
<div class="resources col-md-6">
  <div style="color: #736f7e">
    <h2>Interview Times</h2>
    <h3>
      <%= link_to "Interview Times", interview_times_path %>
    </h3>
  </div>
  <hr>
</div>

