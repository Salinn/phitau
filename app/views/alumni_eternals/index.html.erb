<div class="row">
  <div align="center">
    <h1>Search for an alumni eternal</h1>
    <input type="text" name="FirstName" value="Mickey">
  </div>
  <hr>
  <% @alumni_eternals.each do |alumni_eternal| %>
      <div style="float: left; display: inline-block; padding: 10px;">
        <%= image_tag "http://upload.wikimedia.org/wikipedia/commons/1/13/Daniel_Ingram_Profile.png", style: "display: block;"%>
        <h3 align="center">
          <%= link_to "#{alumni_eternal.first_name} #{alumni_eternal.last_name}", alumni_eternal %>
        </h3>
      </div>
  <% end %>
  <div class="clear"></div>
  <br>
</div>